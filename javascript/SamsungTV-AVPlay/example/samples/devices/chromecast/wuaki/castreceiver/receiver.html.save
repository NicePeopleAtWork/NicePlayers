<!DOCTYPE html>
<html>
<head>
  <title>Wuaki.tv</title>
  <script type="text/javascript" src="http://pre.smartplugin.youbora.com/src/v1/1.2/js/libs/youbora-api.js"></script>
  <script type="text/javascript" src="http://pre.smartplugin.youbora.com/src/v1/1.2/js/libs/youbora-data.js?debug=true"></script>
  <script type="text/javascript" src="http://pre.smartplugin.youbora.com/src/v1/1.2/js/chromecast/sp.js"></script>
  <link rel="shortcut icon" href="favicon.ico">
  <link rel="stylesheet" href="assets/styles.css" />
  <script type="text/javascript" src="./assets/func.js"></script>
  <script type="text/javascript" src="https://www.gstatic.com/cast/sdk/libs/receiver/2.0.0/cast_receiver.js"></script>
  <script type="text/javascript" src="https://www.gstatic.com/cast/sdk/libs/mediaplayer/0.9.0/media_player.js"></script>
  <script type="text/javascript">

  setTimeout( function () {
    youboraData.setPropertyMetaTitle("Tears of steel");
    youboraData.setAccountCode('demosite');
    youboraData.setUsername('Chromecast');
    SmartPlugin.Init();
  } , 2000)
  </script>
</head>
<body data-version="1.12.0">
  <div id="player" class="player">
    <div class="media">
      <video></video>
    </div>
    <div class="overlay">
      <div class="logo"></div>
      <div class="spinit">
        <span data-i18n="ready_to_cast"></span>
      </div>
      <div class="controls">
        <span class="controls-play-pause"></span>
        <span class="controls-cur-time"></span>
        <span class="controls-total-time"></span>
        <div class="controls-progress">
          <div class="controls-progress-inner"></div>
          <div class="controls-progress-thumb"></div>
        </div>
      </div>
      <div class="stamp"></div>
      <div class="media-info">
        <div class="media-description"></div>
        <div class="media-artwork"></div>
        <div class="media-title-director">
          <div class="media-title"></div>
          <div class="media-director"></div>
        </div>
      </div>
      <div class="gradient"></div>
    </div>
  </div>
</body>
</html>

