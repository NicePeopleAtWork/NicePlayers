<?xml version="1.0"?><layout>	<deck id="controls">		<card id="defaultcontrols">			<region id="tpTitleRegion">				<row id="tpTitleContainer">					<control id="tpTitle"/>				</row>			</region>			<region id="tpAdCountdownRegion">				<row id="tpAdCountdownContainer">					<control id="tpAdCountdown"/>				</row>			</region>			<region id="tpBottomRegion">				<row>					<control id="tpPlay"/>					<control id="tpPrevious"/>					<control id="tpNext"/>					<control id="tpVolume"/>					<group percentWidth="100">						<control id="tpScrubber"/>						<spacer width="4"/>						<control id="tpCurrentTime"/>						<control id="tpTimeDivider"/>						<control id="tpTotalTime"/>						<spacer width="4"/>					</group>					<control id="tpBandwidth"/>					<control id="tpFullScreen"/>					<control id="tpMenu"/>				</row>			</region>		</card>	</deck></layout>