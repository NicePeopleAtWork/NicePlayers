<?xml version="1.0"?><controls initialHideDelay="3s" autoHideDelay="2000ms">	<region id="tpTitleRegion">		<row id="tpTitleContainer">			<control id="tpTitle"/>		</row>	</region>	<region id="tpAdCountdownRegion">		<row id="tpAdCountdownContainer">			<control id="tpAdCountdown"/>		</row>	</region>	<region id="tpLeftFloatRegion" autoHide="true">		<column>			<control id="tpPlay"/>			<control id="tpPrevious"/>			<control id="tpNext"/>			<control id="tpVolume"/>		</column>	</region>	<region id="tpRightFloatRegion" autoHide="true">		<column>			<control id="tpCC"/>			<control id="tpBandwidth"/>			<control id="tpFullScreen"/>			<control id="tpEmail"/>			<control id="tpLink"/>		</column>	</region>	<region id="tpBottomFloatRegion" autoHide="true">		<row>			<group percentWidth="100">				<control id="tpScrubber"/>				<spacer width="4"/>				<control id="tpCurrentTime"/>				<control id="tpTimeDivider"/>				<control id="tpTotalTime"/>				<spacer width="4"/>			</group>		</row>	</region></controls>