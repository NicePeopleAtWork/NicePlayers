<?xml version="1.0"?>
<controls>
	<region id="tpAdCountdownRegion">
		<row id="tpAdCountdownContainer">
			<control id="tpAdCountdown"/>
		</row>
	</region>
	<region id="tpBottomFloatRegion" alpha="75">
		<row>
			<control id="tpPlay" showLabel="true" icon="http://mpx.theplatform.com/assets/player.png,http://mpx.theplatform.com/assets/media.png" colorizeIcon="false"/>
			<group percentWidth="100" direction="horizontal" verticalAlign="middle">
				<control id="tpScrubber"/>
				<spacer/>
				<control id="tpCurrentTime"/>
				<control id="tpTimeDivider"/>
				<control id="tpTotalTime"/>
				<spacer/>
			</group>
			<control id="tpMute"/>
			<control id="tpVolumeSlider"/>
			<control id="tpCC"/>
		</row>
	</region>
	<region id="tpRightFloatRegion" alpha="75">
		<column>
			<control id="tpFullScreen" height="70" width="60"/>
			<control id="tpBandwidth" height="40" width="60"/>
			<control id="tpRSS" height="50" width="60"/>
			<control id="tpMenu" height="50" width="60"/>
		</column>
	</region>
	<!--region id="tpTopFloatRegion" alpha="75">



		<row paddingTop="8" paddingBottom="8" paddingLeft="4">



			<control id="tpTitle"/>



		</row>



	</region-->
</controls>
